<h1 class="text-center"><b>Shopping Cart</b></h1>
<hr>

<% @line_items.each do |p| %>
<!-- Product name -->
<%= Product.find(p.product_id).product_name %>
<!-- Product price -->
<%= Product.find(p.product_id).price %>
<!-- Quantity with default 1, adjust later-->
<%= p.quantity %>
<% end %>